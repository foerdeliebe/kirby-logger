{
  "name": "foerdeliebe/kirby-logger",
  "description": "Add a logger to your Kirby website",
  "type": "kirby-plugin",
  "version": "0.9.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Scheurich",
      "email": "support@michaelscheurich.dev"
    }
  ],
  "keywords": [
    "kirby",
    "kirby4",
    "kirby-cms",
    "kirby-plugin",
    "logger",
    "website-log",
    "user-log",
    "panel-log"
  ],
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Michnhokn\\": "classes/"
    }
  },
  "require": {
    "php": ">=8.0.0",
    "getkirby/cms": "^4.0",
    "getkirby/composer-installer": "^1.1"
  },
  "extra": {
    "installer-name": "logger"
  }
}
